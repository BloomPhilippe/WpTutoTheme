{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "wpackagist-plugin/custom-post-type-ui": "1.2.1",
    "wpackagist-plugin/developer": "1.2.6",
    "wpackagist-plugin/disable-comments": "1.3.2",
    "wpackagist-plugin/wp-optimize": "1.8.9.10",
    "wpackagist-plugin/p3-profiler": "1.5.3.9",
    "wpackagist-plugin/regenerate-thumbnails": "2.2.6"
  },
  "extra": {
        "installer-paths": {
            "../../plugins/{$name}/": ["type:wordpress-plugin"]
        }
    }
}
